{
  "name": "bread",
  "displayName": "bread",
  "description": "Leave breakcrumbs across the codebase and have llm type for you inline",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/bra1nDump/bread"
  },
  "engines": {
    "vscode": "^1.81.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "birds.feed",
        "title": "Feed birds with bread"
      }
    ],
    "keybindings": []
  },
  "scripts": {
    "compile": "webpack",
    "watch": "webpack --watch",
    "bundle:prod": "webpack --mode production --devtool false",
    "__comment__": "Using --no-dependencies otherwise vsce complains about extra things being in node_module https://github.com/microsoft/vscode-vsce/issues/421#issuecomment-1038911725",
    "package": "pnpm run bundle:prod && ./node_modules/.bin/vsce package --no-dependencies --allow-star-activation",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint . --ext ts --fix",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.81.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "@vscode/test-electron": "^2.3.2",
    "@vscode/vsce": "^2.20.1",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.85.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "execa": "^7.2.0",
    "langchain": "^0.0.130",
    "openai": "^4.0.0"
  }
}